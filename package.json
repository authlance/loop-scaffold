{
  "private": true,
  "name": "loop",
  "version": "1.0.1",
  "main": "index.ts",
  "scripts": {
    "prepare": "yarn run build",
    "build": "authlance-run build",
    "build:prod": "authlance-run build:prod",
    "build:clean": "authlance-run prepare",
    "build:core": "yarn run build --ignore @loop/browser",
    "start:backend": "node ./examples/browser/src-gen/backend/main.js",
    "publish": "lerna exec --no-private -- npm publish --access public --registry=https://npm.pkg.github.com --scope=@loop",
    "unpublish": "node_modules/.bin/lerna exec --no-private -- npm run unpublish",
    "start:webpackdev": "FORCE_COLOR=3 yarn --cwd examples/browser start:browser",
    "start:dev": "concurrently -n browser,backend -c yellow,green \"yarn --cwd examples/browser start:browser\" \"DEV_PROXY_WEBPACK=true node examples/browser/src-gen/backend/main.js --hostname=0.0.0.0 --port=3000 --config=app-config.json\"",
    "watch:packages": "node ./scripts/lerna.js run watch --parallel --stream --ignore @loop/browser",
    "watch": "concurrently --kill-others -n packages,browser -c blue,yellow \"yarn watch:packages\" \"FORCE_COLOR=3 yarn -s --cwd examples/browser start:browser\""
  },
  "dependencies": {
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.0.1",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@types/express": "^4.16.0",
    "@types/fs-extra": "^4.0.2",
    "@types/node": "^20.11.30",
    "@types/uuid": "^7.0.3",
    "@types/yargs": "^15",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/eslint-plugin-tslint": "^7.0.2",
    "@typescript-eslint/parser": "^5.59.0",
    "@mdx-js/loader": "^3.0.1",
    "assert": "^2.1.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^3.5.0",
    "constants-browserify": "^1.0.0",
    "copyfiles": "^2.4.1",
    "crypto-browserify": "^3.12.1",
    "eslint": "8.19.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.37.4",
    "https-browserify": "^1.0.0",
    "lerna": "^9.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^5.4.5",
    "url": "^0.11.4",
    "util": "^0.12.5",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "resolutions": {
    "trim-newlines": "^3.0.1",
    "dot-prop": "^4.2.1",
    "debug": "^4.3.4",
    "cross-spawn": "^7.0.6"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
